{
  "name": "@demo/monolith",
  "version": "0.0.0",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "npm run prebuild && nest build",
    "start": "nest start",
    "start:es": "env STORE_TYPE=eventstoredb npm run start",
    "start:nats": "env STORE_TYPE=nats npm run start",
    "test": "env JEST_ROOT_DIR=demo/monolith jest --config ../../jest.config.js",
    "test:e2e": "jest --config ./tests/jest-e2e.config.js",
    "test:e2e-es": "env STORE_TYPE=eventstoredb npm run test:e2e",
    "test:e2e-nats": "env STORE_TYPE=nats npm run test:e2e"
  },
  "dependencies": {
    "@nest-cqrs/core": "*",
    "@nest-cqrs/eventstoredb": "*",
    "@nest-cqrs/nats": "~1.0.1",
    "@nestjs/common": "~9.4.0",
    "@nestjs/core": "~9.4.0",
    "@nestjs/platform-express": "~9.4.0",
    "@nestjs/platform-ws": "~9.4.0",
    "@nestjs/websockets": "~9.4.0",
    "class-transformer": "~0.5.1",
    "class-transformer-storage": "*",
    "class-validator": "~0.14.0",
    "nest-messenger": "*",
    "reflect-metadata": "~0.1.13",
    "rxjs": "~7.8.0"
  },
  "devDependencies": {
    "@faker-js/faker": "~7.6.0",
    "@nest-cqrs/testing": "*",
    "@nestjs/cli": "~9.4.0",
    "@nestjs/testing": "~9.4.0",
    "@swc/core": "~1.3.51",
    "@swc/jest": "~0.2.26",
    "@types/express": "~4.17.17",
    "@types/jest": "~29.5.0",
    "@types/supertest": "~2.0.12",
    "@types/ws": "~8.5.4",
    "jest": "~29.5.0",
    "rimraf": "~5.0.0",
    "supertest": "~6.3.3",
    "typescript": "~5.0.4",
    "ws": "~8.13.0"
  }
}
